<div class="container" id="clients-page">
<h1>Satisfied Clients</h1>

<ul id="customer-list" class="clearfix center-block col-xs-12">
<% @clients.each do |c| %>
  <li class="col-sm-3 col-xs-6">
    <h3><%= c.name %></h3>
    <p><%= c.tag %></p>
     <img class="img-responsive" src="<%= c.logo_url %>">
  </li>
<% end %>
</ul>
</div>