<div class="container" id="faq-page">
<h1>Frequently Asked Questions</h1>
<ul>
	<% @faqs.each do |faq| %>
  <li>
    <h3><%= faq.question %></h3>
    <p><%= faq.answer %></p>

    
    <%if user_signed_in? && is_admin? %>
    <%= link_to "Delete", faq, method: :delete, class: "delete_link", remote: true %>
    <%= link_to "Edit", edit_faq_path(faq), class: "edit_link" %>
    <% end %>


  </li>
<% end %>
</ul %>

</div>
